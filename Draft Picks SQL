SELECT * FROM picks;

/*Select all wide recievers*/
SELECT * FROM picks WHERE position= 'WR';
/*How many wide reicvers were drafted? */
SELECT COUNT(*) AS TOTAL_WR_DRAFTED FROM picks WHERE position='WR';
/* How many of each postition were drafted?*/
SELECT position, COUNT(position) AS 'drafts_per_position' FROM picks GROUP BY position;
/* What college had the most draft picks?*/
SELECT college, COUNT(college) AS 'picks_per_college' 
FROM picks GROUP by college;
/* Where did Kansas City draft from?*/
SELECT college AS 'KC_college_drafts' FROM picks WHERE picked_by='Kansas City';
/* What the average pick number of a offensive tackle?*/
SELECT AVG(pick_number) AS 'OT_avg_pick_number' FROM picks WHERE position= 'OT';
/*What is the average pick number per position? */
SELECT position, avg(pick_number) AS 'avg_pick_number' FROM picks GROUP BY position;
